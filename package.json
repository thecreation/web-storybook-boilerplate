{
  "version": "0.0.5",
  "name": "web-boilerplate",
  "author": "Creation Studio Limited",
  "license": "MIT",
  "repository": "https://github.com/thecreation/web-boilerplate",
  "bugs": {
    "url": "https://github.com/thecreation/web-boilerplate/issues"
  },
  "homepage": "https://github.com/thecreation/web-boilerplate",
  "scripts": {
    "start": "npm-run-all --sequential build --parallel watch storybook",
    "dev": "NODE_ENV=dev scripts/dev",
    "build": "npm-run-all img svg icons vendor css:compile js:compile",
    "dist": "NODE_ENV=prod npm-run-all img svg vendor css js",
    "clean": "npm-run-all clean:*",
    "clean:css": "scripts/clean-css",
    "clean:js": "scripts/clean-js",
    "clean:img": "scripts/clean-img",
    "clean:svg": "scripts/clean-svg",
    "clean:vendor": "scripts/clean-vendor",
    "css": "npm-run-all css:beautify css:compile css:minify",
    "css:beautify": "scripts/beautify-css",
    "css:compile": "scripts/compile-css",
    "css:minify": "scripts/minify-css",
    "js": "npm-run-all js:beautify js:compile js:minify",
    "js:beautify": "scripts/beautify-js",
    "js:compile": "scripts/compile-js",
    "js:minify": "scripts/minify-js",
    "icons": "scripts/generate-icons",
    "img": "npm-run-all img:minify",
    "img:minify": "scripts/minify-img",
    "svg": "npm-run-all svg:minify",
    "svg:minify": "scripts/minify-svg",
    "lint": "npm-run-all lint:css:fix lint:js:fix lint:md",
    "lint:css": "scripts/lint-css",
    "lint:css:fix": "scripts/lint-css --fix",
    "lint:js": "scripts/lint-js",
    "lint:js:fix": "scripts/lint-js --fix",
    "lint:md": "scripts/lint-md",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "scripts/watch-js",
    "watch:css": "scripts/watch-css",
    "vendor": "scripts/vendor",
    "release": "release-it",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "picturefill": "^3.0.3"
  },
  "browserslist": [
    "> 1%",
    "ie >= 11",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/html": "^5.1.9",
    "assets-manager": "^0.4.1",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "chalk": "^2.4.2",
    "colors": "^1.3.3",
    "cssnano": "^4.1.10",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-compat": "^3.3.0",
    "front-matter": "^3.0.2",
    "fs-extra": "^8.1.0",
    "gaze": "^1.1.3",
    "glob-expand": "^0.2.1",
    "glob-parent": "^5.0.0",
    "globby": "^10.0.1",
    "husky": "^3.0.1",
    "image-size": "^0.7.4",
    "imagemin": "^7.0.0",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-webp": "^5.1.0",
    "jsonlint": "^1.6.3",
    "lint-staged": "^9.2.0",
    "markdownlint": "^0.15.0",
    "node-sass": "^4.12.0",
    "node-sass-import": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.17",
    "postcss-csso": "^3.0.0",
    "postcss-header": "^1.0.0",
    "postcss-reporter": "^6.0.1",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.18.2",
    "release-it": "^12.3.3",
    "rename": "^1.0.4",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sass-loader": "^7.1.0",
    "semver": "^6.2.0",
    "sharp": "^0.22.1",
    "stringify-attributes": "^2.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.1",
    "stylelint-scss": "^3.9.2",
    "svgo": "^1.3.0",
    "uglify-es": "^3.3.9",
    "usemin": "^0.5.0",
    "webfont": "^9.0.0",
    "yargs": "^13.3.0",
    "yargs-parser": "^13.1.1"
  }
}
